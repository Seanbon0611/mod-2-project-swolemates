<img src="<%= @member.img_url %>">
<h1><%= @member.name %></h1>
<h3><%= @member.email %></h3>
Age: <%= @member.age %><br>
Address: <%= @member.address %>
<br><br>
My Gyms:<br>
<ul>
    <% @member.gyms.each do |g| %>
        <li><%= link_to g.name, g %></li>
    <% end %>
</ul>
My Coaches:<br>
<ul>
    <% @member.coaches.each do |c| %>
        <li><%= link_to c.name, c %></li>
    <% end %>
</ul>